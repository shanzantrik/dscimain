{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "storage/app/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/storage/app/public/$1"
        },
        {
            "src": "/(css|js|images|videos|fonts|favicon.ico|robots.txt)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)\\.(?:css|js|jpg|jpeg|gif|png|ico|svg|webp|mp4)$",
            "dest": "/public/$1.$2",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
                "Content-Security-Policy": "upgrade-insecure-requests",
                "X-Content-Type-Options": "nosniff",
                "X-XSS-Protection": "1; mode=block",
                "X-Frame-Options": "SAMEORIGIN",
                "Referrer-Policy": "strict-origin-when-cross-origin"
            }
        }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://dscimain-9sq2z50ve-shaanjyots-projects.vercel.app",
        "ASSET_URL": "https://dscimain-9sq2z50ve-shaanjyots-projects.vercel.app",
        "FORCE_HTTPS": "true",
        "LOG_CHANNEL": "stderr",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "array",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH": "node"
    }
}
